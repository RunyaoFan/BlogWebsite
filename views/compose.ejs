<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="newTitle">
    <label>Post</label>
    <textarea name="newContent" rows="6" cols="80" class="form-control"></textarea>
  </div>

  <button type="submit" name="button" class="btn-primary btn">Publish</button>
</form>
<%- include("partials/footer"); -%>
